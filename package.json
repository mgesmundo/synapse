    "cov": "mocha test/test.js --require blanket --reporter html-cov > test/coverage.html"
  },
  "dependencies": {
    "debug": "~2.0.0",
    "underscore": "~1.7.0",
    "axon-rpc": "~0.0.3",
    "axon-secure": "~2.0.6",
    "ip": "~0.3.1",
    "multicast-events": "~2.1.2",
    "port-manager": "~1.0.0",
    "useit": "~1.2.0",
    "vargs": "~0.1.0",
    "normalize-arguments": "~1.2.0"
  },
  "devDependencies": {
    "mocha": "~1.21.4",
    "should": "~4.0.4",
    "sinon": "~1.10.3",
    "blanket": "~1.1.6"
  },
  "config": {
    "blanket": {
      "pattern": "synapsejs/lib/"
    }
  },
  "analyze": false,
  "license": "BSD",
  "engines": {
    "node": ">=0.10"
  },
  "readmeFilename": "README.md"
