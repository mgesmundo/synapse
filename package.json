{
  "name": "synapsejs",
  "description": "Distributed RPC server and client.",
  "preferGlobal": "false",
  "version": "1.3.2",
  "author": {
    "name": "Marcello Gesmundo",
    "email": "mgesmundo@yoovant.com",
    "url": "http://www.yoovant.com"
  },
  "keywords": [
    "rpc",
    "synapse",
    "events",
    "multicast",
    "network"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgesmundo/synapse.git"
  },
  "bugs": {
    "url": "https://github.com/mgesmundo/synapse/issues"
  },
  "main": "index.js",
  "scripts": {
